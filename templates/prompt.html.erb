<style>
    @media print {
        body, html {
            height: 100%;
            background-color: white;
            margin: 0;
            padding: 0;
        }

        .card-container {
            width: 100%;
            box-sizing: border-box;
            padding: 1cm;
            gap: 0.5cm;
        }

        .logo {
            text-align: center;
        }
    }

    body {
        font-family: 'Helvetica', sans-serif;
        display: flex;
        justify-content: center;
        align-items: center;
        background-color: white;
    }

    .card-container {
        display: grid;
        grid-template-columns: repeat(4, 1fr);
        gap: 0;
    }

    .card {
        background-color: white;
        border: 1px solid black;
        border-radius: 1px;
        aspect-ratio: 7/8;
        display: flex;
        justify-content: center;
        align-items: center;
        position: relative;
    }

    .card-content {
        display: flex;
        flex-direction: column;
        align-items: center;
        justify-content: center;
        width: 80%;
        height: 80%;
    }

    .logo {
        margin-top: auto;
        display: inline-block;
        width: 100%;
        text-align: center;
    }
</style>
